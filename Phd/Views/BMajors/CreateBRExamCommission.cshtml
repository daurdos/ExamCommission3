@model Phd.Models.BRExamCommission

@{
    ViewData["Title"] = "Создать ГАК";
}

<h2>Создать</h2>

<h4>ГАК</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateBRExamCommission">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="hidden" value="@ViewBag.Id" asp-for="BMajorId" />
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Создать" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-sm btn-primary" asp-action="Index">Домой</a>
</div>

